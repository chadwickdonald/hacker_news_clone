 <% if signed_in?  %>
    <li class='nav-collapse nav'>
      <%= link_to "Sign out", signout_path %>
    </li>

    <li class='offset4'>
      <%= "Logged in as #{current_user.username}."%>
    </li>

  <% else %>
    <li class='nav-collapse nav'>
      <%= link_to "Sign in #{current_user}", new_session_path %>
    </li>
    <li class='nav-collapse nav'>
    <%= link_to "Sign Up", new_user_path %>
    </li>
  <% end %>